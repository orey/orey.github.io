@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix ga: <https://orey.github.io/graphapps-V2#> .

@prefix exs: <https://orey.github.io/sample-schema.ttl#> .

exs:ClassA rdfs:subClassOf ga:Class .

exs:ClassB rdfs:subClassOf exs:ClassA .

exs:ClassC rdfs:subClassOf ga:Class .

exs:attribute1 rdfs:subPropertyOf ga:attribute ;
               rdfs:domain exs:ClassA ;
               rdfs:range rdf:langString .

exs:attribute2 rdfs:subPropertyOf ga:attribute ;
               rdfs:domain exs:ClassA ;
               rdfs:range exs:ClassC .






